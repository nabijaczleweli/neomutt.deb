#: main.c:278
msgid "  neomutt -v[v]\n"
msgstr "  neomutt -v[v]\n"

#. L10N: Try to limit to 80 columns.  If more space is needed add an indented line
#: main.c:281
msgid "options:"
msgstr "seçenekler:"

#: main.c:282
msgid ""
"  --            Special argument forces NeoMutt to stop option parsing and treat\n"
"                remaining arguments as addresses even if they start with a dash"
msgstr ""
"  --            Özel bir argüman, NeoMutt'un ayrıştırmayı durdurmasını ve\n"
"                  kalan argümanları tireyle başlasa bile adres gibi görmesini\n"
"                  sağlar"

#: main.c:284
msgid "  -A <alias>    Print an expanded version of the given alias to stdout and exit"
msgstr "  -A <arma>     Verilen armanın uzun sürümünü stdout'a yazdır ve çık"

#: main.c:285
msgid ""
"  -a <file>     Attach one or more files to a message (must be the last option)\n"
"                Add any addresses after the '--' argument"
msgstr ""
"  -a <dosya>    İletiye bir/birden çok dosya iliştir (son seçenek olmalıdır)\n"
"                '--' argümanından sonra herhangi bir adresi ekle"

#: main.c:287
msgid "  -B            Run in batch mode (do not start the ncurses UI)"
msgstr "  -B            Toplu iş kipinde çalıştır (ncurses arabirimini çalıştırma)"

#: main.c:288
msgid "  -b <address>  Specify a blind carbon copy (Bcc) recipient"
msgstr "  -b <adres>    Bir gizli karbon kopya (Bcc) alıcısı belirt"

#: main.c:289
msgid "  -c <address>  Specify a carbon copy (Cc) recipient"
msgstr "  -c <adres>    Bir karbon kopya (Cc) alıcısı belirt"

#: main.c:290
msgid "  -D            Dump all config variables as 'name=value' pairs to stdout"
msgstr ""
"  -D            Tüm yapılandırma değişkenlerini 'ad=değer' ikilisi olarak\n"
"                  stdout'a boşalt"

#: main.c:291
msgid "  -D -O         Like -D, but show one-liner documentation"
msgstr "  -D -O         -D gibi; ancak tek satırlık belgelendirme göster"

#: main.c:292
msgid "  -D -S         Like -D, but hide the value of sensitive variables"
msgstr "  -D -S         -D gibi; ancak hassas değişkenlerin değerini gizle"

#: main.c:293
msgid ""
"  -d <level>    Log debugging output to a file (default is \"~/.neomuttdebug0\")\n"
"                The level can range from 1-5 and affects verbosity"
msgstr ""
"  -d <düzey>    Hata ayıklama çıktısını dosyaya günlük olarak yaz\n"
"                  (öntanımlı: \"~/.neomuttdebug0\")\n"
"                  Düzey: 1-5 arası, buna göre açıklama derecesi ayarlanır"

#: main.c:295
msgid "  -E            Edit draft (-H) or include (-i) file during message composition"
msgstr "  -E            Taslağı düzenle (-H) veya ileti oluşturma sırasında içer (-i)"

#: main.c:296
msgid "  -e <command>  Specify a command to be run after reading the config files"
msgstr "  -e <komut>    Yapılandırma dosyasını okuyup çalıştırılacak bir komut belirt"

#: main.c:297
msgid "  -F <config>   Specify an alternative initialization file to read"
msgstr "  -F <yapıl.>   Okunacak alternatif bir ilklendirme dosyası belirt"

#: main.c:298
msgid "  -f <mailbox>  Specify a mailbox (as defined with 'mailboxes' command) to load"
msgstr "  -f <postak.>  Yüklenecek posta kutusu belirt ('mailboxes' komutuna göre)"

#: main.c:299
msgid "  -G            Start NeoMutt with a listing of subscribed newsgroups"
msgstr "  -G            NeoMutt'ı abone haber gruplarının bir listesiyle başlat"

#: main.c:300
msgid "  -g <server>   Like -G, but start at specified news server"
msgstr "  -g <sunucu>   -G gibi; ancak belirtilen haber sunucusunda başlat"

#: main.c:301
msgid "  -H <draft>    Specify a draft file with header and body for message composing"
msgstr "  -H <taslak>   İleti oluşturma için üstbilgi ve gövdeden oluşan bir taslak belirt"

#: main.c:302
msgid "  -h            Print this help message and exit"
msgstr "  -h            Bu yardım iletisini yazdır ve çık"

#: main.c:303
msgid "  -i <include>  Specify an include file to be embedded in the body of a message"
msgstr "  -i <içer>     İletinin gövdesine gömülecek bir içerme dosyası belirt"

#: main.c:304
msgid "  -l <file>     Specify a file for debugging output (default \"~/.neomuttdebug0\")"
msgstr ""
"  -l <dosya>    Hata ayıklama çıktısı için bir dosya belirt\n"
"                  (öntanımlı: \"~/.neomuttdebug0\")"

#: main.c:305
msgid ""
"  -m <type>     Specify a default mailbox format type for newly created folders\n"
"                The type is either MH, MMDF, Maildir or mbox (case-insensitive)"
msgstr ""
"  -m <tür>      Yeni oluşturulan klasörler için öntanımlı posta kutusu biçim türü\n"
"                  belirt. Tür NH, MMDF, Maildir veya mbox (BÜYÜK/küçük harf\n"
"                  duyarsız) olabilir"

#: main.c:307
msgid "  -n            Do not read the system-wide configuration file"
msgstr "  -n            Sistem geneli yapılandırma dosyasını okuma"

#: main.c:308
msgid "  -p            Resume a prior postponed message, if any"
msgstr "  -p            Daha önceden ertelenen bir ileti varsa sürdür"

#: main.c:309
msgid ""
"  -Q <variable> Query a configuration variable and print its value to stdout\n"
"                (after the config has been read and any commands executed)\n"
"                Add -O for one-liner documentation"
msgstr ""
"  -Q <değişken> Bir yapılandırma değişkeni sorgula ve değerini stdout'a yazdır\n"
"                  (yapılandırma dosyası okunduktan ve komutların çalışmasından\n"
"                  sonra); tek satırlık belgelendirme için -O ekle"

#: main.c:312
msgid "  -R            Open mailbox in read-only mode"
msgstr "  -R            Posta kutusunu saltokunur kipte aç"

#: main.c:313
msgid "  -s <subject>  Specify a subject (must be enclosed in quotes if it has spaces)"
msgstr "  -s <konu>     Bir konu belirt (boşluk varsa tırnak içinde olmalıdır)"

#: main.c:314
msgid "  -v            Print the NeoMutt version and compile-time definitions and exit"
msgstr "  -v            NeoMutt sürümünü ve derleme tanımlarını yazdırıp çık"

#: main.c:315
msgid "  -vv           Print the NeoMutt license and copyright information and exit"
msgstr "  -vv           NeoMutt lisansını ve telif hakkı bilgisini yazdırıp çık"

#: main.c:316
msgid "  -y            Start NeoMutt with a listing of all defined mailboxes"
msgstr "  -y            NeoMutt'ı tüm tanımlı posta kutularının bir listesiyle başlat"

#: main.c:317
msgid ""
"  -Z            Open the first mailbox with new message or exit immediately with\n"
"                exit code 1 if none is found in all defined mailboxes"
msgstr ""
"  -Z            Yeni ileti içeren ilk posta kutusunu aç veya tanımlı posta\n"
"                  kutularında yeni ileti yoksa 1 çıkış koduyla çık"

#: main.c:319
msgid ""
"  -z            Open the first or specified (-f) mailbox if it holds any message\n"
"                or exit immediately with exit code 1 otherwise"
msgstr ""
"  -z            İleti varsa ilk veya tanımlanan (-f) posta kutusunu aç veya bir\n"
"                  ileti yoksa 1 çıkış koduyla çık"

